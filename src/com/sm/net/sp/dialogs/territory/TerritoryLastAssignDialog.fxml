<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sm.net.sp.dialogs.territory.TerritoryLastAssignDialog">
   <children>
      <VBox fx:id="memberAssignedTerritoryTablesVBox" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox>
               <children>
                  <StackPane>
                     <children>
                        <Label fx:id="preacherLabel" text="proclamatore">
                           <StackPane.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </StackPane.margin>
                        </Label>
                     </children>
                  </StackPane>
                  <StackPane HBox.hgrow="ALWAYS">
                     <children>
                        <TextField fx:id="preacherTextField">
                           <StackPane.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </StackPane.margin>
                        </TextField>
                     </children>
                  </StackPane>
                  <StackPane>
                     <children>
                        <Button fx:id="preacherButton" mnemonicParsing="false" text="proclamatore">
                           <StackPane.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </StackPane.margin>
                        </Button>
                     </children>
                  </StackPane>
               </children>
            </HBox>
            <StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
               <children>
                  <TableView fx:id="tableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                     <columns>
                        <TableColumn fx:id="territoryNrTableColumn" prefWidth="75.0" text="numero" />
                        <TableColumn fx:id="territoryNameTableColumn" prefWidth="75.0" text="territorio" />
                        <TableColumn fx:id="lastDoneTableColumn" prefWidth="75.0" text="ultima lavorazione" />
                        <TableColumn fx:id="preacherHadTableColumn" prefWidth="75.0" text="lavorato dal proclamatore" />
                        <TableColumn fx:id="preacherHadDateTableColumn" prefWidth="75.0" text="lavorato dal proclamatore il" />
                     </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <StackPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </StackPane.margin>
                  </TableView>
               </children>
            </StackPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
