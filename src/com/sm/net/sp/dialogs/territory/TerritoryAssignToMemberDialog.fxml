<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sm.net.sp.dialogs.territory.TerritoryAssignToMemberDialog">
   <children>
      <VBox fx:id="memberAssignedTerritoryTablesVBox">
         <children>
            <StackPane>
               <children>
                  <TextField fx:id="filterTextField">
                     <StackPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </StackPane.margin>
                  </TextField>
               </children>
            </StackPane>
            <StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
               <children>
                  <TableView fx:id="membersTableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                     <columns>
                        <TableColumn fx:id="memberIconTableColumn" prefWidth="75.0" text="simbolo" />
                        <TableColumn fx:id="memberSurnameTableColumn" prefWidth="75.0" text="cognome" />
                        <TableColumn fx:id="memberNameTableColumn" prefWidth="75.0" text="nome" />
                     </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <StackPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </StackPane.margin>
                  </TableView>
               </children>
            </StackPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
