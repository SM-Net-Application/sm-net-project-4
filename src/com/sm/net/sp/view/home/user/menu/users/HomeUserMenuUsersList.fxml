<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sm.net.sp.view.home.user.menu.users.HomeUserMenuUsersList">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <StackPane alignment="CENTER_LEFT">
               <children>
                  <Label fx:id="userLabel" maxHeight="1.7976931348623157E308" text="Label">
                     <StackPane.margin>
                        <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
                     </StackPane.margin>
                  </Label>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </StackPane>
            <StackPane maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS">
               <children>
                  <TableView fx:id="userTableView" maxHeight="1.7976931348623157E308">
                    <columns>
                        <TableColumn fx:id="userTableColumnID" prefWidth="75.0" text="Column X" />
                      <TableColumn fx:id="userTableColumnName" prefWidth="75.0" text="C1" />
                      <TableColumn fx:id="userTableColumnRoleAdmin" prefWidth="75.0" text="C2" />
                        <TableColumn fx:id="userTableColumnUsers" prefWidth="75.0" text="C2" />
                        <TableColumn fx:id="userTableColumnCongregations" prefWidth="75.0" text="C2" />
                        <TableColumn fx:id="userTableColumnServiceGroups" prefWidth="75.0" text="C2" />
                        <TableColumn fx:id="userTableColumnMeetings" prefWidth="75.0" text="C2" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <StackPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </StackPane.margin>
                  </TableView>
               </children>
            </StackPane>
            <HBox>
               <children>
                  <StackPane alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS">
                     <children>
                        <Button fx:id="userAddButton" mnemonicParsing="false" text="Button">
                           <StackPane.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </StackPane.margin>
                        </Button>
                     </children>
                  </StackPane>
                  <StackPane>
                     <children>
                        <Button fx:id="userDeleteButton" mnemonicParsing="false" text="Button">
                           <StackPane.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </StackPane.margin>
                        </Button>
                     </children>
                  </StackPane>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
