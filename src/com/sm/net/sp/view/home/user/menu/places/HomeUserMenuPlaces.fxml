<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sm.net.sp.view.home.user.menu.places.HomeUserMenuPlaces">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox maxHeight="75.0" minHeight="75.0" prefHeight="75.0">
               <children>
                  <StackPane>
                     <children>
                        <ImageView fx:id="headerImageView" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                           <StackPane.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </StackPane.margin>
                        </ImageView>
                     </children>
                  </StackPane>
                  <StackPane alignment="CENTER_LEFT">
                     <children>
                        <Label fx:id="headerLabel" text="Label">
                           <StackPane.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </StackPane.margin>
                        </Label>
                     </children>
                  </StackPane>
               </children>
            </HBox>
            <Separator>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Separator>
            <HBox>
               <children>
                  <StackPane alignment="CENTER_RIGHT">
                     <children>
                        <Button fx:id="addButton" mnemonicParsing="false" text="Button">
                           <StackPane.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </StackPane.margin>
                        </Button>
                     </children>
                  </StackPane>
                  <StackPane>
                     <children>
                        <Button fx:id="deleteButton" mnemonicParsing="false" text="Button">
                           <StackPane.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </StackPane.margin>
                        </Button>
                     </children>
                  </StackPane>
               </children>
            </HBox>
            <StackPane VBox.vgrow="ALWAYS">
               <children>
                  <TableView fx:id="placesTableView" maxHeight="1.7976931348623157E308">
                    <columns>
                        <TableColumn fx:id="typeTableColumn" prefWidth="75.0" text="tipo" />
                      <TableColumn fx:id="descrTableColumn" prefWidth="75.0" text="denom" />
                        <TableColumn fx:id="streetTableColumn" prefWidth="75.0" text="strada" />
                        <TableColumn fx:id="numTableColumn" prefWidth="75.0" text="n" />
                        <TableColumn fx:id="postCodeTableColumn" prefWidth="75.0" text="cap" />
                        <TableColumn fx:id="cityTableColumn" prefWidth="75.0" text="citta" />
                        <TableColumn fx:id="countyTableColumn" prefWidth="75.0" text="provincia" />
                        <TableColumn fx:id="countryTableColumn" prefWidth="75.0" text="nazione" />
                        <TableColumn fx:id="coordTableColumn" prefWidth="75.0" text="coord" />
                        <TableColumn fx:id="defaultTableColumn" prefWidth="75.0" text="predef" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <StackPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </StackPane.margin>
                  </TableView>
               </children>
            </StackPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
