<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sm.net.sp.view.home.user.menu.dateandtime.HomeUserMenuDateAndTime">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox maxHeight="75.0" minHeight="75.0" prefHeight="75.0">
               <children>
                  <StackPane>
                     <children>
                        <ImageView fx:id="headerImageView" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                           <StackPane.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </StackPane.margin>
                        </ImageView>
                     </children>
                  </StackPane>
                  <StackPane alignment="CENTER_LEFT">
                     <children>
                        <Label fx:id="headerLabel" text="Label">
                           <StackPane.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </StackPane.margin>
                        </Label>
                     </children>
                  </StackPane>
               </children>
            </HBox>
            <StackPane>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
               <children>
                  <Separator>
                     <StackPane.margin>
                        <Insets />
                     </StackPane.margin>
                  </Separator>
               </children>
            </StackPane>
            <HBox>
               <children>
                  <StackPane alignment="CENTER_RIGHT">
                     <children>
                        <Button fx:id="addButton" mnemonicParsing="false" text="Button">
                           <StackPane.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </StackPane.margin>
                        </Button>
                     </children>
                  </StackPane>
                  <StackPane>
                     <children>
                        <Button fx:id="deleteButton" mnemonicParsing="false" text="Button">
                           <StackPane.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </StackPane.margin>
                        </Button>
                     </children>
                  </StackPane>
               </children>
            </HBox>
            <StackPane>
               <children>
                  <Separator>
                     <StackPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </StackPane.margin>
                  </Separator>
               </children>
            </StackPane>
            <StackPane VBox.vgrow="ALWAYS">
               <children>
                  <TableView fx:id="dateTimeTableView" maxHeight="1.7976931348623157E308">
                    <columns>
                        <TableColumn fx:id="dateTimeStartDayTableColumn" prefWidth="75.0" text="data" />
                      <TableColumn fx:id="dateTimeDay1TableColumn" prefWidth="75.0" text="giornoinfr" />
                        <TableColumn fx:id="dateTimeHours1TableColumn" prefWidth="75.0" text="orainfr" />
                        <TableColumn fx:id="dateTimeDay2TableColumn" prefWidth="75.0" text="giornofs" />
                        <TableColumn fx:id="dateTimeHours2TableColumn" prefWidth="75.0" text="orafs" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <StackPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </StackPane.margin>
                  </TableView>
               </children>
            </StackPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
