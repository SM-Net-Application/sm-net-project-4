<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="layout" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sm.net.sp.view.historyaudio.HistoryAudio">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox VBox.vgrow="ALWAYS">
               <children>
                  <VBox>
                     <children>
                        <StackPane alignment="CENTER_LEFT">
                           <children>
                              <Label fx:id="titleLabel" text="Label">
                                 <StackPane.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </StackPane.margin>
                              </Label>
                           </children>
                        </StackPane>
                        <StackPane>
                           <children>
                              <Separator>
                                 <StackPane.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </StackPane.margin>
                              </Separator>
                           </children>
                        </StackPane>
                        <StackPane VBox.vgrow="ALWAYS">
                           <children>
                              <TableView fx:id="membersTableView">
                                <columns>
                                  <TableColumn fx:id="memberNameTableColumn" prefWidth="75.0" text="brother" />
                                  <TableColumn fx:id="memberLastDateTableColumn" prefWidth="75.0" text="last assignment" />
                                    <TableColumn fx:id="memberLastDateStatusTableColumn" prefWidth="75.0" text="status" />
                                </columns>
                                 <StackPane.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </StackPane.margin>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                           </children>
                        </StackPane>
                     </children>
                  </VBox>
                  <StackPane>
                     <children>
                        <Separator orientation="VERTICAL">
                           <StackPane.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </StackPane.margin>
                        </Separator>
                     </children>
                  </StackPane>
                  <VBox HBox.hgrow="ALWAYS">
                     <children>
                        <StackPane alignment="CENTER_LEFT">
                           <children>
                              <Label fx:id="brotherLabel" text="Label">
                                 <StackPane.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </StackPane.margin>
                              </Label>
                           </children>
                        </StackPane>
                        <StackPane>
                           <children>
                              <Separator>
                                 <StackPane.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </StackPane.margin>
                              </Separator>
                           </children>
                        </StackPane>
                        <StackPane VBox.vgrow="ALWAYS">
                           <children>
                              <TableView fx:id="privilegesTableView">
                                 <columns>
                                    <TableColumn fx:id="privilegesDateTableColumn" prefWidth="75.0" text="date" />
                                    <TableColumn fx:id="privilegesStatusTableColumn" prefWidth="75.0" text="status" />
                                    <TableColumn fx:id="privilegesNameTableColumn" prefWidth="75.0" text="name" />
                                 </columns>
                                 <StackPane.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </StackPane.margin>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                           </children>
                        </StackPane>
                        <StackPane>
                           <children>
                              <TextArea fx:id="notesTextArea">
                                 <StackPane.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </StackPane.margin>
                              </TextArea>
                           </children>
                        </StackPane>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <StackPane alignment="CENTER_RIGHT">
               <children>
                  <Button fx:id="selectButton" mnemonicParsing="false" text="Button">
                     <StackPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </StackPane.margin>
                  </Button>
               </children>
            </StackPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
