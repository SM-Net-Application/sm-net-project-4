<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sm.net.sp.view.menu.settings.user.SettingUser">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <StackPane>
               <children>
                  <Label fx:id="titleLabel" text="Label">
                     <StackPane.margin>
                        <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
                     </StackPane.margin>
                  </Label>
               </children>
            </StackPane>
            <StackPane alignment="CENTER_LEFT">
               <children>
                  <Label fx:id="usernameLabel" text="Label">
                     <StackPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </StackPane.margin>
                  </Label>
               </children>
            </StackPane>
            <StackPane alignment="CENTER_LEFT">
               <children>
                  <TextField fx:id="usernameTextField">
                     <StackPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </StackPane.margin>
                  </TextField>
               </children>
            </StackPane>
            <StackPane alignment="CENTER_LEFT">
               <children>
                  <Label fx:id="passwordLabel" text="Label">
                     <StackPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </StackPane.margin>
                  </Label>
               </children>
            </StackPane>
            <StackPane alignment="CENTER_LEFT">
               <children>
                  <PasswordField fx:id="passwordField">
                     <StackPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </StackPane.margin>
                  </PasswordField>
               </children>
            </StackPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
